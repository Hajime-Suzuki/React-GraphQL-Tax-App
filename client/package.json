    "gen:compile": "tsc ./src/graphql/typeDefs.ts --outDir ./_gql-gen/ --target ES5",
    "gen": "gql-gen",
    "gen:debug": "gql-gen DEBUG=true",
    "addRedirects": "echo '/* /index.html 200' > ./build/_redirect"
